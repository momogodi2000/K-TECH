    "date-fns": "^3.3.1",
    "react-hot-toast": "^2.4.1",
    "react-intersection-observer": "^9.8.0",
    "react-lazy-load-image-component": "^1.6.0",
    "swiper": "^11.0.5",
    "i18next": "^23.8.2",
    "react-i18next": "^14.0.5",
    "react-ga4": "^2.1.0",
    "vite": "^5.1.0",
    "@vitejs/plugin-react": "^4.2.1",
    "vite-plugin-pwa": "^0.17.5",
    "tailwindcss": "^3.4.1",
